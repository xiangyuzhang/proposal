set terminal postscript eps 'Times-Roman' 20
set output  "c1355_Corrupt_Random_incre_time.eps" 

# configurate size below

set key center at 35, 100
set style data lines
set xlabel "Number of Camouflaged gates"
set ylabel "CPU Time (s)"
set xrange[0:200]
set logscale y
set key samplen 2
set key width 0.1
set grid y
set size 1,0.4
set label "C1355" at 105, 300

plot  "incremental_Time_MUX4_all.dat" using 1:($8+$9)/2:($9-$8)/2 title "" w errorbars linestyle 1, "incremental_Time_MUX4_all.dat" using 1:($8+$9)/2 title 'Random median' w linespoints pt 0 ps 1 lc -1 lw 4
